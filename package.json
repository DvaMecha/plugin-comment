{
    "name": "gitbook-plugin-comment",
    "description": "Inline comments with gitbook.com",
    "main": "index.js",
    "version": "0.1.8",
    "license": "Apache-2.0",
    "homepage": "https://github.com/GitbookIO/plugin-comment",
    "engines": {
        "gitbook": ">=2.5.0"
    },
    "devDependencies": {
        "less": "2.5.1",
